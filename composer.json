{
    "name": "elapse-annals/laravel-plus",
    "type": "project",
    "description": "The Laravel Plus Framework.",
    "keywords": [
        "framework",
        "laravel-plus"
    ],
    "license": "Apache-2.0",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "fideloper/proxy": "^4.0",
        "guzzlehttp/guzzle": "^6.3",
        "laravel/framework": "^6.0",
        "laravel/socialite": "^4.1",
        "laravel/tinker": "^1.0",
        "maatwebsite/excel": "3.1.x-dev",
        "overtrue/laravel-lang": "^3.0",
        "rap2hpoutre/laravel-log-viewer": "^1.1",
        "respect/validation": "^1.1"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "beyondcode/laravel-dump-server": "^1.0",
        "beyondcode/laravel-self-diagnosis": "^1.2",
        "filp/whoops": "^2.0",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "mpociot/laravel-apidoc-generator": "^3.11",
        "nunomaduro/collision": "^3.0",
        "nunomaduro/larastan": "^0.3.17",
        "overtrue/laravel-query-logger": "^1.0",
        "phpunit/phpunit": "^8.5",
        "phptool/laravel": "dev-master"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
